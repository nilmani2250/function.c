//design a function to check given number is prime or not.If return 1 else return 0.//

#include<stdio.h>
int prime_check(int);
void main()
{
int num,r;
printf("Enter the number...\n");
scanf("%d",&num);
r=prime_check(num);
if(r==1)
printf("yes prime...\n");
else
printf("No not prime...\n");
}

int prime_check(int n)
{

int i,c=0;
for(i=1;i<=n;i++)
{
if(n%==0)
c++;
}
if(c==2)
}
