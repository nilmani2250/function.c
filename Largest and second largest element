// Design a function to find out largest and second largest element in a given array.//

#include<stdio.h>
void find_L_SL(int*,int,int*,int*);
void main()
{
int a[5]={10,200,3,400,60);
int ele,l,sl;
ele=sizeof(a)/sizeof(a[0]);
find_L_SL(a,ele,&l,&sl);
printf("l=%d sl=%d\n",l,sl);
}
void find_L_SL9int *a,int ele,int*p,,int*q)
{
int l,sl,i;
if(a[0]>a[1])
{
l=a[0];
sl=a[1];
}
else
{
l=a[1]
sl=a[0];
}
for(i=2;i<ele;i++)
{
if(a[i]>l)
{
sl=l;
l=a[i];
else if(a[i[>sl && a[i]!=l)
sl=a[i];
}
*p=l
*q=sl
}
